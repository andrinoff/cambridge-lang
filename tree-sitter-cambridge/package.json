{
  "name": "tree-sitter-cambridge",
  "version": "0.1.5",
  "description": "Tree-sitter grammar for Cambridge International AS & A Level Pseudocode",
  "main": "bindings/node",
  "types": "bindings/node",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrinoff/cambridge-lang.git"
  },
  "keywords": [
    "tree-sitter",
    "parser",
    "cambridge",
    "pseudocode"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "andrinoff",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.0",
    "prebuildify": "^6.0.0"
  },
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "tree-sitter": [
    {
      "scope": "source.cambridge",
      "file-types": [
        "pseudo",
        "psc"
      ],
      "highlights": "queries/highlights.scm"
    }
  ]
}
